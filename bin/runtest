#!/usr/bin/bash

# For new users who forget:
sudo ./setroot
sudo ./setdir

cd .. && cd scripts
. ./rip-environment 
# Clean previous hal session:
./halrun -U

cd ..
cd bin

# Test:
halcmd stop
halcmd loadrt threads name1=base-thread fp1=0 period1=25000 name2=servo-thread period2=1000000
halcmd loadrt test
halcmd addf the_function servo-thread
halcmd start
halcmd show



